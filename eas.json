{
  "cli": {
    "version": ">= 0.46.0"
  },
  "build": {
    "production": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "development": {
      "extends": "production",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "master": {
      "extends": "production",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "master"
      }
    },
    "staging": {
      "extends": "production",
      "env": {
        "NODE_ENV": "staging"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "1609750373"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    },
    "staging": {
      "ios": {
        "ascAppId": "1609750373"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
